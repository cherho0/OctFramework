@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Modal.cshtml";
}

@using Oct.Framework.TestWeb.Areas.Premission.Models;

@model CommonRoleInfoDTO

<div class="container">
    <div class="form form-horizontal">
        @Html.HiddenFor(d => d.Id)		 

        <div class="form-group">
            <label class="col-xs-3 control-label"><i class="fa fa-asterisk required"></i>Name</label>
            <div class="col-xs-5 control-label">
                @Model.Name
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-3 control-label"><i class="fa fa-asterisk required"></i>Code</label>
            <div class="col-xs-5 control-label">
                @Model.Code
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-3 control-label"><i class="fa fa-asterisk required"></i>IsEnable</label>
            <div class="col-xs-5 control-label">
                @Model.IsEnable
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-3 control-label"><i class="fa fa-asterisk required"></i>是否系统默认角色</label>
            <div class="col-xs-5 control-label">
                @Model.IsSysDefault
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-3 control-label"><i class="fa fa-asterisk required"></i>CreateDate</label>
            <div class="col-xs-5 control-label">
                @Model.CreateDate
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-3 control-label"><i class="fa fa-asterisk required"></i>ModifyDate</label>
            <div class="col-xs-5 control-label">
                @Model.ModifyDate
            </div>
        </div>
    </div>
</div>
